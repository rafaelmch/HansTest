    <TypeScriptConfiguration Remove="src\app\NovaPasta1\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="src\app\NovaPasta1\**" />
  </ItemGroup>
  <ItemGroup>
    <TypeScriptConfiguration Remove="src\app\NovaPasta2\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="src\app\NovaPasta2\**" />
  </ItemGroup>
</Project>